{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run build && prisma generate",
                "installCommand": "npm install && npx prisma generate",
                "prisma": "generate"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
