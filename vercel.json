{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run build && npx prisma generate",
                "env": {
                    "PRISMA_CLIENT_ENGINE_TYPE": "binary"
                }
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
